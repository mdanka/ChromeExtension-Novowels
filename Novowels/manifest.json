{
  "name": "Novowels",
  "description": "Erase vowels from all paragraphs",
  "version": "2.0",
  "permissions": [
	"activeTab"
  ],
  "web_accessible_resources": [
	"change_paragraphs.js"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "icons": {
	"128": "icon.png"
  },
  "browser_action": {
  "default_icon": "icon.png",
    "default_title": "Erase vowels from all paragraphs"
  },
  "manifest_version": 2
}
	